  .dac-tutorial-page .dac-preview-toggle-wrapper .dac-preview-toggle + .dac-preview-phone { display: none; padding-top: 40px; margin: auto; width: 200px; }
  .dac-tutorial-page .dac-preview-toggle-wrapper .dac-preview-toggle.toggle + .dac-preview-phone { display: block; }
  .dac-tutorial-page .dac-preview-toggle-wrapper .dac-preview-phone, .dac-tutorial-page .dac-preview-toggle-wrapper .hide, .dac-tutorial-page .dac-preview-toggle-wrapper.toggle .show { display: none !important; }
  .dac-tutorial-page .dac-preview-toggle-wrapper .show, .dac-tutorial-page .dac-preview-toggle-wrapper.toggle .dac-preview-phone, .dac-tutorial-page .dac-preview-toggle-wrapper.toggle .hide { display: block !important; }
  .dac-tutorial-page .dac-preview-toggle-wrapper .show::after, .dac-tutorial-page .dac-preview-toggle-wrapper.toggle .hide::after { font-style: normal; font-variant: normal; font-kerning: auto; font-optical-sizing: auto; font-variation-settings: normal; font-weight: normal; font-stretch: normal; font-size: 24px; line-height: 24px; font-family: "Material Icons"; font-feature-settings: "liga"; -webkit-font-smoothing: antialiased; text-rendering: optimizelegibility; text-transform: none; overflow-wrap: normal; height: 23px; position: absolute; width: 50px; }
  .dac-tutorial-page .dac-preview-toggle-wrapper .show::after { content: "expand_more"; }
  .dac-tutorial-page .dac-preview-toggle-wrapper.toggle .hide::after { content: "expand_less"; }
}

